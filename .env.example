# Sentry (Error Tracking)
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
NEXT_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Security
# ========================================
# JWT Secret for session tokens
JWT_SECRET=your_super_secret_jwt_key_min_32_chars
# Session timeout in minutes
SESSION_TIMEOUT=30
# Max login attempts before lockout
MAX_LOGIN_ATTEMPTS=5

# Rate Limiting
# ========================================
RATE_LIMIT_WINDOW=60000
# Time window in milliseconds (60000 = 1 minute)
RATE_LIMIT_MAX_REQUESTS=100
# Max requests per window

# File Upload (Optional)
# ========================================
# Max file size in bytes (10MB default)
MAX_FILE_SIZE=10485760
# Allowed file types
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Redis (Optional - for caching/sessions)
# ========================================
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password

# AWS S3 (Optional - for file storage)
# ========================================
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-bucket-name

# Webhook URLs (for payment callbacks)
# ========================================
PAYU_WEBHOOK_URL=https://workflow.gsapps.in/api/webhooks/payu
CASHFREE_WEBHOOK_URL=https://workflow.gsapps.in/api/webhooks/cashfree

# Feature Flags (Optional)
# ========================================
ENABLE_SIGNUPS=true
REQUIRE_EMAIL_VERIFICATION=true
ENABLE_2FA=false
ENABLE_OAUTH=true
ENABLE_PAYMENTS=true

# Development Only
# ========================================
# Set to 'true' to bypass authentication in development
DEV_MODE=false
# Set to 'true' to see detailed error messages
DEBUG=false
